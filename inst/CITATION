citHeader("To cite AustralianElections in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(
  bibtype  = "Misc",
  title    = "{AustralianElections}: Provides Datasets About Australian Elections",
  author   = c(person("Rohan", "Alexander"),
               person("Paul", "Hodgetts", "Alexander")),
  year     = year,
  note     = note,
  url      = "https://github.com/RohanAlexander/AustralianElections"
  )

bibentry(
  bibtype  = "Misc",
  title    = "Australian Election Archive",
  author   = person("Adam", "Carr"),
  year     = year,
  url      = "http://psephos.adam-carr.net/countries/a/australia/"
  )
